{
  "name": "volume-viewer",
  "version": "2.0.0-alpha.11",
  "description": "volume renderer for multichannel 8-bit intensity data stored as 3D arrays",
  "main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "type-declarations",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build-docs": "node node_modules/documentation/bin/documentation.js build src/Histogram.js src/View3d.js src/Volume.js src/VolumeMaker.js src/volumeLoader.js -f html -o docs --shallow",
    "build": "npm run transpileES",
    "build-demo": "webpack --config webpack.demo.js",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "lint": "eslint --config ./.eslintrc --ignore-path ./.eslintignore --ext .jsx --ext .js ./",
    "test": "mocha --opts mocha.opts src/**/test/*.js",
    "transpileES": "babel src --out-dir es --extensions .ts,.js --ignore src/**/*.test.ts"
  },
  "author": "Daniel Toloudis",
  "license": "MIT",
  "dependencies": {
    "three": "^0.99.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "~7.2",
    "@babel/preset-env": "~7.3",
    "@babel/preset-typescript": "~7.1",
    "@types/chai": "~4.1",
    "@types/mocha": "~5.2",
    "@typescript-eslint/eslint-plugin": "~1.1",
    "@typescript-eslint/parser": "~1.1",
    "babel-loader": "~8.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "chai": "~4.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "documentation": "^8.1.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "mocha": "~5.2",
    "nyc": "~13.1",
    "prettier": "^1.16.1",
    "raw-loader": "^0.5.1",
    "rimraf": "~2.6",
    "ts-node": "~8.0",
    "typescript": "~3.2",
    "url-loader": "^1.1.2",
    "webpack": "~4.29",
    "webpack-cli": "~3.2",
    "webpack-dev-server": "^3.8.0",
    "worker-loader": "^1.0.0"
  }
}
